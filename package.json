{
  "name": "samune",
  "version": "0.0.1",
  "description": "Generator of thumbnails",
  "main": "build/index.js",
  "scripts": {
    "test": "node_modules/mocha/bin/_mocha --timeout 15000 --require 'espower-coffee/guess' test/**/*.coffee",
    "setup": "npm i && npm run compile",
    "compile": "node_modules/coffee-script/bin/coffee -o build/ src/",
    "dev": "node_modules/coffee-script/bin/coffee -o build/ -w src/",
    "install": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eiurur/Samune.git"
  },
  "keywords": [
    "thumbnail",
    "ImageMagicK"
  ],
  "author": "eiurur",
  "license": "MIT",
  "dependencies": {
    "imagemagick": "^0.1.3",
    "lodash": "^4.6.1",
    "mkdirp": "^0.5.1",
    "randomstring": "^1.1.4",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "espower-coffee": "^1.0.1",
    "espower-loader": "^1.0.0",
    "intelli-espower-loader": "^1.0.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "power-assert": "^1.2.0",
    "rimraf": "^2.5.2"
  }
}
